<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MovieApp.Views.MovieDetailsPage"
             Title="MovieDetailsPage">
    <StackLayout Padding="10">
        <Image Source="{Binding PosterUrl}" HeightRequest="300" WidthRequest="500" />
        <Label Text="{Binding Name}" FontSize="Large" FontAttributes="Bold" Margin="10" />
        <StackLayout Orientation="Horizontal" Margin="10, 0, 10, 10">
            <!-- Ngôi sao thứ 1 -->
            <Image Source="data:image/png;base64," WidthRequest="20" HeightRequest="20" />
            <!-- Ngôi sao thứ 2 -->
            <Image Source="data:image/png;base64," WidthRequest="20" HeightRequest="20" />
            <!-- Ngôi sao thứ 3 -->
            <Image Source="data:image/png;base64," WidthRequest="20" HeightRequest="20" />
            <!-- Ngôi sao thứ 4 -->
            <Image Source="data:image/png;base64," WidthRequest="20" HeightRequest="20" />
            <!-- Ngôi sao thứ 5 -->
            <Image Source="data:image/png;base64," WidthRequest="20" HeightRequest="20" />
        </StackLayout>

        <Label Text="{Binding Genre, StringFormat='Thể loại: {0}'}" FontSize="Medium" FontAttributes="Bold" Margin="5" />
        <Label Text="{Binding Actors, StringFormat='Diễn viên: {0}'}" FontSize="Small" FontAttributes="Bold" Margin="5" />
        <Button Text="Thêm vào danh sách yêu thích" FontSize="Medium" TextColor="White" BorderWidth="10"  ContentLayout="right, 10"/>
        <Label Text="{Binding Description}" FontSize="Small" Margin="10" />
    </StackLayout>
</ContentPage>